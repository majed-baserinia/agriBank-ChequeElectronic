{
  "plugins": [
    "prettier-plugin-organize-attributes",
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"
  ],
  "tabWidth": 2,
  "printWidth": 90,
  "singleAttributePerLine": true,
  "trailingComma": "none",
  "htmlWhitespaceSensitivity": "strict",
  "tailwindFunctions": ["tw"]
}
